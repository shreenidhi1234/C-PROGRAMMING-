#include<stdio.h>

#include<conio.h>

int main()

{ 

	float pi=3.14;	int r = 6;

    float area,cr;

    printf("\n the radius of the circle");

	area= (float)(pi*r*r);

	printf("\nArea of the circle %0.2f",area);

	cr=(float)2*pi*r;

	printf("\nthe circumference of the circle : %0.2f",cr);

	getch();

	return 0;

}
